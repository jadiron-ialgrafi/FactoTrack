<div class="card nestable-cart">
    <div class="card-title">
        <h4>{{ knob.title }}</h4>
    </div>
    <div class="card-body text-center">
        <div class="superpose">
            <div class="superclock">
                <div class="superclock1">
                    <input class="knob hour" 
                           data-min="0" 
                           data-max="24" 
                           data-bgColor="#EEEEEE" 
                           data-fgColor="#FB6E52" 
                           data-displayInput="false" 
                           data-width="300" 
                           data-thickness=".3">
                </div>
                <div class="superclock2">
                    <input class="knob minute" 
                           data-min="0" 
                           data-max="60" 
                           data-bgColor="#EEEEEE" 
                           data-fgColor="#48CFAE" 
                           data-displayInput="false" 
                           data-width="200" 
                           data-thickness=".45">
                </div>
                <div class="superclock3">
                    <input class="knob second" 
                           data-min="0" 
                           data-max="60" 
                           data-bgColor="#EEEEEE" 
                           data-fgColor="#50C1E9" 
                           data-displayInput="false" 
                           data-width="100" 
                           data-thickness=".3">
                </div>
            </div>
        </div>
    </div>
</div>